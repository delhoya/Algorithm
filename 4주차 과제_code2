class Dlist:
    
    class node: 
        def __init__(self,item,prev,link):
    
            self.item = item
            self.prev = prev
            self.link = link
        
    def __init__(self):
        
        self.head = None
        self.tail = None
       # self.head = self.node(none,none,none)
       # self.tail = self.node(none,self.head,none)
        self.size = 0
    
    def size(self):
        return self.size
    
    def isempty(self):
        return self.size ==0
    
    def insert_before(self,p,item):
        temp = p.prev
        new_node = self.node(item,temp,p)
        p.prev = new_node
        temp.next = new_node
        self.size += 1 
        
    def delete (self,x):
        f = x.prev
        r = x.next
        f.next = r
        r.prev = f
        self.size -= 1
        return x.item
    
    def print_list(self):
        if self.isempty():
            print('empty')
        
        else: 
            p = self.head.next
            while p!= self.tail:
                if p.next != self.tail:
                    print(p.item,end ='')
                else: 
                    print(p.item)
                p = p.next # 노드 차례대로 방문
                
class Emptyerror(Exception):
    pass
